    "def nth_largest_element(arr, n):\n",
    "    \n",
    "    check =[]\n",
    "    for x in range (n):\n",
    "        check.append(max(arr))\n",
    "        max_val = arr.index(max(arr))\n",
    "        arr.pop(max_val)\n",
    "        \n",
    "    return check[n-1]\n",
    "\n",
    "nth_largest_element([1,2,3,4,5,6], 4)"
